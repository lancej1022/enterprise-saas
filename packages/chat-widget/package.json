    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "jsdom": "^26.0.0",
    "prettier": "3.6.2",
    "tailwindcss": "^4.0.6",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-no-bundle": "4.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "3.0.5"

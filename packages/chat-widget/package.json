    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/router.js",
      "require": "./dist/umd/router.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite dev --port 3001",

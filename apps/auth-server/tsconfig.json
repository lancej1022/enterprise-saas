{
  "extends": ["@solved-contact/tsconfig/base.json"],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "noEmit": false,
    "baseUrl": "./",
    "paths": {
      "#/*": ["./src/*"]
    },
    "outDir": "./dist",
    "types": ["node"],
    "composite": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "tsc-alias": {
    "resolveFullPaths": true
  },
  "exclude": ["node_modules", "build", "dist"]
}
